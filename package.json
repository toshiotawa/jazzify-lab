{
  "name": "jazz-learning-game",
  "version": "1.0.0",
  "description": "Jazz Learning Game - Music game with piano and guitar modes",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:perf": "vite build --mode production --logLevel warn",
    "build:fast": "vite build --mode production",
    "build:check": "tsc && vite build",
    "build:analyze": "vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "vite build --mode production && vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:migrate": "supabase db diff --use-migra -f new_migration",
    "supabase:push": "supabase db push",
    "supabase:pull": "supabase db pull",
    "supabase:types": "supabase gen types typescript --local > src/types/supabase.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@supabase/supabase-js": "^2.50.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "opensheetmusicdisplay": "^1.9.0",
    "pixi.js": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tonal": "^6.4.2",
    "tone": "^14.7.77",
    "zod": "^3.25.75",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "supabase": "^2.30.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
