{
  "name": "jazz-learning-game",
  "version": "1.0.0",
  "description": "Jazz Learning Game - Music game with piano and guitar modes",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "build:perf": "vite build --mode production --logLevel warn",
    "build:fast": "vite build --mode production",
    "build:ultra-fast": "vite build --mode production --logLevel error --force",
    "build:dev": "vite build --config vite.config.dev.ts",
    "build:dev-fast": "vite build --config vite.config.dev.ts --logLevel error",
    "build:check": "tsc && vite build",
    "build:analyze": "vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "vite build --mode production && vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules/.vite node_modules && npm install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@heroicons/react": "^2.2.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/supabase-js": "^2.39.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.6",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "marked": "^16.0.0",
    "opensheetmusicdisplay": "^1.9.0",
    "pixi.js": "^7.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.49.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.6.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "tonal": "^6.4.2",
    "tone": "^14.7.77",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "stripe": "^14.25.0",
    "supabase": "^2.30.4",
    "tailwindcss": "^3.4.0",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
